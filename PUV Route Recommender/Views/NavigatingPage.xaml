<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PUV_Route_Recommender.Views.NavigatingPage"
             Title="NavigatingPage">
    <VerticalStackLayout>
        <Frame 
            Margin="20, 10, 20, 0"
            Padding="20, 2, 20, 2"
            CornerRadius="45">

            <SearchBar
            Placeholder="Origin"/>
        </Frame>
        
        <Frame 
            Margin="20, 10, 20, 0"
            Padding="20, 2, 20, 2"
            CornerRadius="45">

            <SearchBar
                Placeholder="Destination"
                Style="None"/>
        </Frame>

        

    </VerticalStackLayout>
</ContentPage>